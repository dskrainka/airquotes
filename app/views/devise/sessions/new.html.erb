<div class="form">

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


<form>

  <div class="group">
    <input id="user_email" name="user[email]" type="email" readonly onfocus="this.removeAttribute('readonly');"><span class="highlight"></span><span class="bar"></span>
    <label>Email</label>
  </div>

  <div class="group">
    <input id="user_password" name="user[password]" type="password" readonly onfocus="this.removeAttribute('readonly');"><span class="highlight"></span><span class="bar"></span>
    <label>Password</label>
  </div>
  <button name="commit" type="submit" class="button buttonBlue" value ="Create Session">Sign in
    <div class="ripples buttonRipples"><span class="ripplesCircle"></span></div>
  </button>
</form>

  <% end %> 



</div>

